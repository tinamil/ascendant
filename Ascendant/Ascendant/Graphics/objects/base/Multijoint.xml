<?xml version="1.0" encoding="UTF-8"?>
<model type="dynamic">
  <mesh>Diamond.obj</mesh>
  <material>
    <file>Diamond.mtl</file>
    <type>diamond</type>
  </material>
  <point_lights>
    <light>Cube.light</light>
  </point_lights>
  <scale>
    <x>1</x>
    <y>1</y>
    <z>1</z>
  </scale>
  <position>
    <x>-1</x>
    <y>1</y>
    <z>0</z>
  </position>
  <orientation>
    <x>0</x>
    <y>1</y>
    <z>0</z>
    <degrees>0</degrees>
  </orientation>
  <mass>5</mass>
  <!--<momentum> defaults to zero-->
  <!--<angular_momentum> defaults to zero-->
  <children>
    <child>
      <model>Cube.xml</model>
      <constraint type="conetwist">
        <swingX>90</swingX>
        <swingY>90</swingY>
        <twist>0</twist>
        <!--Optional-->
        <!--<softness></softness>
        <bias></bias>
        <relaxation></relaxation>-->

        <referenceParent>
          <position>
            <x>-1</x>
            <y>1</y>
            <z>0</z>
          </position>
          <orientation>
            <x>0</x>
            <y>1</y>
            <z>0</z>
            <degrees>0</degrees>
          </orientation>
        </referenceParent>
        <referenceChild>
          <position>
            <x>1</x>
            <y>0</y>
            <z>0</z>
          </position>
          <orientation>
            <x>1</x>
            <y>1</y>
            <z>1</z>
            <degrees>0</degrees>
          </orientation>
        </referenceChild>
      </constraint>
    </child>
  </children>
</model>